{
  "manifest_version": 2,

  "name": "Save Opened Tabs",
  "description": "This extension save URLs and title of opened tans to the clipboard.",
  "version": "1.0",

  "permissions": [
    "tabs"
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_title": "Save and Load Tabs",
    "default_popup": "popup.html"
  }
}
